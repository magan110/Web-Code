<script>
if("N" == "Y" && 1 != 1)
{
	$('#myModalSchm').modal('show');
	$(document).ready(function()
	{
		$('#myModalSchm').modal({
			backdrop: 'static'
		});
	});

	//document.getElementById("dataSave").style.visibility = "visible";
	setTimeout(function()
	{
