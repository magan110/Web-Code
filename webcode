<script>

function dashBoardStrt()
{
	presentLoading();
//	setTimeout(function () { $('.circle').fadeIn(); }, 50);
	const url = "DashBoardHm.jsp";

	$(function() 
	{
		$.get(url, 
		{
			crossDomain: true,
			cache: 'default',
		// mode : 'login',
		},
			function(responseText, status) 
			{
				document.getElementById("some-menu").innerHTML = responseText;

				if(status == "success");
				{
					dismissLoader();
				}
				
			}, "text");
	});
}
//dashBoardStrt();
function custCrdData()
{
//	setTimeout(function () { $('.circle').fadeIn(); }, 50);
	const url = "/BirlaWhite/Mobile/custHmDsBrd.jsp?custCode=2948&slDatTyp=";
	$.ajax(
	{
		url:url,    //the page containing php script
		type: "GET",    //request type,
		success:function(reponse, data)
		{
		//	console.log(reponse.trim());
	//		console.log(data.trim());
			var cstCrdLt = reponse.trim().split("~~");
			//	alert(status);
				if(data.trim() == "success");
				{
					if(cstCrdLt[1] == "Msg")
					{
						var cstData = cstCrdLt[2].split("~");
						document.getElementById("crdLmt").innerHTML = cstData[1];
						document.getElementById("opnBling").innerHTML = cstData[11];
						document.getElementById("opnOrdr").innerHTML = cstData[7];
	//						alert(cstData[13]);
						if(cstData[13] == "undefined" || cstData[13] == null)
							document.getElementById("BlncLmt").innerHTML = "";
						else
							document.getElementById("BlncLmt").innerHTML = cstData[13];

						 var chrtData =  [cstData[1], cstData[13], cstData[7], cstData[9], cstData[11]];
						 var chrtLabl =  [cstData[0], cstData[12], cstData[6], cstData[8], cstData[10]];
						/*	chartsSc = [
									 {
										title: '',
										config: 
										{
											type: 'donut',
											defaultLabel: 'No Label',
											parentLabels: true, 
											onclick: function(e, point) 
											{
												console.log('clicked point', point);
											}
										},
										
										data: [
										{
											v: cstData[13],
											l: cstData[12]
										},
										{
											v: cstData[7],
											l: cstData[6]
										},
										{
											v: cstData[9],
											l: cstData[8]
										},
										{
											v: cstData[11],
											l: cstData[10]
										},
										]
									}
									];
								 */
					genChartLc("chartValCrdSc", chrtData, chrtLabl,"bar");
					
				}
				
			}
		},
		error: function (err)
		{
			console.log(err);
		}
	});
}
function custCrdDataSl(slDatTyp)
{
//	setTimeout(function () { $('.circle').fadeIn(); }, 50);
	const url = "/BirlaWhite/Mobile/custHmDsBrd.jsp?custCode=2948&slDatTyp=" + slDatTyp;
	$.ajax(
	{
		url:url,    //the page containing php script
		type: "GET",    //request type,
		success:function(reponse, data)
		{
		//	console.log(reponse.trim());
	//		console.log(data.trim());
			var cstCrdLt = reponse.trim().split("~~");
			//	alert(status);
			if(data.trim() == "success");
			{
				if(cstCrdLt[1] == "Msg")
				{
					//White Cement~7.850~Wall Care Putty~3.900~VAP~0.000~Total Sale~11.75
					var cstData = cstCrdLt[2].split("~");
					
					// alert(cstCrdLt[2]);
					
					if(slDatTyp == "SB")
					{
						//Total Retailer~547~Total Billed~221~Total Billed % ~40.40~WC Billed~136~WC Billed %~24.86~WCP Billed~193~WCP Billed %~35.28~VAP Billed~9~VAP Billed %~1.65
						document.getElementById("totlRetl").innerHTML = cstData[1];
						document.getElementById("totlRetlBild").innerHTML = cstData[3];
						document.getElementById("wcRtl" + slDatTyp).innerHTML = cstData[7];
						document.getElementById("wcpRtl" + slDatTyp).innerHTML = cstData[11];
						document.getElementById("vapRtl" + slDatTyp).innerHTML = cstData[15];
						document.getElementById("lqdprm" + slDatTyp).innerHTML = cstData[19];
						document.getElementById("shpPrm" + slDatTyp).innerHTML = cstData[23];
						document.getElementById("disPrm" + slDatTyp).innerHTML = cstData[27];
						var chrtData =  [cstData[7], cstData[11], cstData[15], cstData[19], cstData[23], cstData[27]];
						var chrtLabl =  [cstData[6], cstData[10], cstData[14], cstData[18], cstData[22], cstData[26]];
					}
					else
					{
					//	document.getElementById("totlSl" + slDatTyp).innerHTML = cstData[7];
						// document.getElementById("wcSal" + slDatTyp).innerHTML = cstData[1];
						// document.getElementById("wcpSal" + slDatTyp).innerHTML = cstData[3];
						// document.getElementById("vapSal" + slDatTyp).innerHTML = cstData[5];
						// document.getElementById("lqdprm" + slDatTyp).innerHTML = cstData[7];
						// document.getElementById("shpPrm" + slDatTyp).innerHTML = cstData[9];
						// document.getElementById("disPrm" + slDatTyp).innerHTML = cstData[11]; // Commented by Navin 27.07.2023 added below code 
						document.getElementById("wcSal" + slDatTyp).innerHTML = cstData[1];
						document.getElementById("shpPrm" + slDatTyp).innerHTML = cstData[7];
						document.getElementById("wcpSal" + slDatTyp).innerHTML = cstData[3];
						document.getElementById("vapSal" + slDatTyp).innerHTML = cstData[5];
						document.getElementById("lqdprm" + slDatTyp).innerHTML = cstData[9];
						document.getElementById("disPrm" + slDatTyp).innerHTML = cstData[11];
						var chrtData =  [cstData[1], cstData[3], cstData[5], cstData[7], cstData[9], cstData[11]];
						var chrtLabl =  [cstData[0], cstData[2], cstData[4], cstData[6], cstData[8], cstData[10]];
					}


					 if(slDatTyp == "P")
						genChartLc("chartValPrimSl", chrtData, chrtLabl, "line");
					 else if(slDatTyp == "S")
						 genChartLc("chartValSecnSl", chrtData, chrtLabl, "bar");
					 else if(slDatTyp == "SB") 
						 genChartLc("chartValRetlSl", chrtData, chrtLabl, "pie");
				}
			}
		},
		error: function (err)
		{
			console.log(err);
		}
	});
}


if("" == "C" && "S" == "M")
{
	custCrdData();
	setTimeout(function()
	{
		custCrdDataSl("P");
	}, 1000);
	setTimeout(function()
	{
		custCrdDataSl("S");
	}, 2000);
	setTimeout(function()
	{
		custCrdDataSl("SB");
	}, 3000);
}
