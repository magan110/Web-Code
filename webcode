<script>
var popAlow = "N";
function createModalHd(hedarVl, urlVarVl)
{
	createModal(hedarVl, urlVarVl);
	//window.parent.createModal(hedarVl, urlVarVl);
}

	
function skipFriendshipPopup()
{
	if("false" == "true")
		$('#myModal').modal('hide');
	
	var datetime_Friendship = new Date();
	var dd_Friendship = datetime_Friendship.getDate();
	var mm_Friendship = datetime_Friendship.getMonth() + 1;
	var yyyy_Friendship = datetime_Friendship.getFullYear();
	datetime_Friendship = dd_Friendship + '/' + mm_Friendship + '/' + yyyy_Friendship;
	localStorage.isFriendshipPopUp = datetime_Friendship;        
	$('#myModal').modal('hide');
}

function dataSaveFc()
{
	if (!$("#chkCnfrm").is(":checked")) 
	{
		alert("Please click on I read this document carefully.");
		return false;
	}
	const url = "https://www.birlawhite.com:8080/Envr/JSP/FileDeleteS.jsp?docRedNo=Y";
	var docRedNo = "";
	document.LoggedInCom.method = "POST";
	document.LoggedInCom.action = "https://www.birlawhite.com:8080/Envr/JSP/FileDeleteS.jsp?docRedNo=Y";
	document.LoggedInCom.submit();
		   /*
	$(function() 
	{
		$.post(url, 
		{
			crossDomain: true,
			docRedNo : "Y",
			cache: 'default',
		},
		function(responseText) 
		{
			alert("responseText : " + responseText);
			if(responseText == "Y")
			{
				refreshed();
			}
			else
			{
				alert("Contact To IT");
			}

		}, "text");
	});
	*/

}
function refreshed()
{
	document.LoggedInCom.method = "POST";
	document.LoggedInCom.action = "LoggedInCom.jsp";
	document.LoggedInCom.submit();
}

